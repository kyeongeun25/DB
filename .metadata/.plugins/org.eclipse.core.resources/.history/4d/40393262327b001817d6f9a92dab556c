package com.biz.exam;

public class Ex03 {

	public static void main(String[] args) {

		// 정수형 배열
		int[] intNums = new int[10] ;	// 10개의 정수형 배열 생성
		// 모든 요소의 값이 자동으로 0으로 초기화 된다.
		// 숫자형 배열은 생성만하면 자동으로 모든 값이 초기화 되기 때문에 읽기가 가능
		
		// 배열이름[위치] : 접근할때는 배열의 위치를 숫자값으로 지정해서 접근한다.
		// 위치 값 : 배열의 첨자
		
		// 배열은 for를 이용해서 접근이 가능하다.
		for(int i = 0 ; i < intNums.length ; i++) {
			i = intNums[i] ;
		}
		
		// 무조건 0부터 intNums.length -1까지 모든 배열을 순회
		for(int i : intNums) {
			
		}
		
		// 변수는 선언을하고 초기화를 하지 않으면
		// 값을 읽을 수 없다.
		int intNum1 ;		// 아직 읽기가 불가능
		intNum1 = 0 ;		// 이때부터 읽기가 가능. 초기화
		
		

	}

}
